@{
    ViewBag.Title = "协议合同登记";
    ViewBag.PageCode = "C2003";
}
@model ContractMvcWeb.Models.Beans.ContractCG 



<div class="content panel panel-default" >
    <div class="panel-heading">
        <label>协议合同登记</label>
    </div>

    <div class="panel-body">

        @using (Html.BeginForm("AddContract", "CGContract", FormMethod.Post ,new { role = "form", @class = "form-horizontal" }))
        {
            @Html.AntiForgeryToken()
            var items = (List<SelectListItem>)ViewData["payprogressItems"];
            
            
                 <div class="form-group">
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">合同编号</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                                @Html.TextBoxFor(m => m.contractnum, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;"><span style="color:red">*</span>采购编号</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.seq, new { @class = "form-control" })
                            </div>
                        </div>
                       <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">采购部门</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.department, new { @class = "form-control" })
                            </div>
                        </div>
      </div>
    
     <div class="form-group">
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">采购联系人</label>
                            <div class="col-sm-2  col-md-3" style="padding-left:5px;">
                                @Html.TextBoxFor(m => m.linker, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">采购类型</label>
                            <div class="col-sm-2  col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.type, new { @class = "form-control" })
                            </div>
                        </div>
                       <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;"><span style="color:red">*</span>采购内容</label>
                            <div class="col-sm-2  col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.content, new { @class = "form-control" })
                            </div>
                        </div>
      </div>
            
       
    
     <div class="form-group">
                        <div class="group">
                            <label class="col-sm-2  col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">联系电话</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                                @Html.TextBoxFor(m => m.tel, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">项目编号</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.projectnum, new { @class = "form-control" })
                            </div>
                        </div>
                       <div class="group">
                            <label class="col-sm-2  col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">经费来源</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.fundsource, new { @class = "form-control" })
                            </div>
                        </div>
      </div>
            
       <div class="form-group">
                        <div class="group">
                            <label class="col-sm-2  col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">数量</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                                @Html.TextBoxFor(m => m.count, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">单价</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.price, new { @class = "form-control" })
                            </div>
                        </div>
                       <div class="group">
                            <label class="col-sm-2  col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">小计</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor( m=>m.subtotal, new { @class = "form-control" })
                            </div>
                        </div>
      </div>
    
     <div class="form-group">
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">总价</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                                @Html.TextBoxFor(m => m.total, new { @class = "form-control"})
                            </div>
                        </div>
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">供货单位</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.super, new { @class = "form-control" })
                            </div>
                        </div>
                       <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">供货联系人</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.superlinker, new { @class = "form-control" })
                            </div>
                        </div>
      </div>
    
    <div class="form-group">
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">联系电话</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                                @Html.TextBoxFor(m => m.supertel, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">结算金额</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.settleamount, new { @class = "form-control" })
                            </div>
                        </div>
                       <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">赠送内容</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.freecontent, new { @class = "form-control" })
                            </div>
                        </div>
      </div>
    
    
     <div class="form-group">
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">赠送价值</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                                @Html.TextBoxFor(m => m.freevalue, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">验收情况</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.validate, new { @class = "form-control" })
                            </div>
                        </div>
                       <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">记录位置</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.place, new { @class = "form-control" })
                            </div>
                        </div>
      </div>
    <div class="form-group">
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">支付进度</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                                @Html.DropDownListFor(m => m.payprogress, items, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">分管部门</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.chargedepartment, new { @class = "form-control" })
                            </div>
                        </div>
                       <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">备注</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.remark, new { @class = "form-control" })
                            </div>
                        </div>
      </div>
            
       <div class="form-group">
             <div class="group">
                 <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">采购时间</label>
                 <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                     @Html.TextBoxFor(m => m.buytime , new { @class = "form-control" })
                 </div>          
             </div>
            <div class="group">
                            <label class="col-sm-2 col-md-1 control-label small" style="padding-right:5px;padding-left:0px;">预算金额</label>
                            <div class="col-sm-2 col-md-3" style="padding-left:5px;">
                               @Html.TextBoxFor(m => m.budgetamount, new { @class = "form-control" })
                            </div>
                        </div>
      </div>      
            
    <div class="form-group">
        <div class="col-sm-12 col-md-12 text-center">
            <button id="btnSave" value="保存" class="btn btn-info" type="submit">保存</button>
            </div>
    </div>
    
             

