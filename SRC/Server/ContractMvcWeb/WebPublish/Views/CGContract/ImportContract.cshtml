@{
    ViewBag.Title = "采购合同导入";
    ViewBag.PageCode = "C2002";
}

<div class="content1 " style="height:500px">
    <link style="text/css" rel="stylesheet" href="~/Content/style2.css" />
<h4>采购合同导入</h4>

    <div class="import_contract_box ">
        <div class="temp_download">
            <ul>
                <li class="li_txt">下载批量导入数据模板</li>
                <li class="li_pic">
                    <p class ="type_icon"></p>
                    <p class="temp_name">模板_批量导入数据.xls</p>
                </li>
                <li >
                    <a class="blue_btn link_common" href="~/CGContract/DownloadExcelTemplate">下载</a>
                </li>
            </ul>
        </div>
        <div class="import_con_middle">
       <a class="right_arrow add mb10"></a>
   </div>
        <div class="temp_upload">
            <form id="uploadForm" method="post" action="~/CGContract/ImportContract" enctype="multipart/form-data" >
                <ul>
                    <li class="li_txt">上传批量数据</li>
                    <li class="temp_load">
                        <p>
                            <input type="text" id="fileName" class="text_input w250 mr10" readonly="readonly" name="fileName">
                            <input type="button" id="selectFile" onclick="import_selectfile()" class="blue_btn button_common" value="浏览">
                            <input type="file" id="uploadFile" onchange="import_changefile()" class="fileupload" name="uploadFile">
                        </p>
                        <p><label>文件名称：</label><span id="displayName" ></span></p>
                    </li>
                    <li>
                        <input type="submit" id="uploadBtn" class="blue_btn button_common mr20" value="上传" />
                        @*<input type="button" id="returnBtn" value="返回" class="gray_btn button_common">*@
                    </li>
                </ul>
            </form>
        </div>

<div class="error_box">@Html.ValidationSummary()</div>

   </div>



</div>